{
    "python.formatting.provider": "black",
    "cSpell.words": [
        "airpods",
        "astype",
        "backgroundcolor",
        "barh",
        "CMTE",
        "dask",
        "dataframe",
        "dropna",
        "dtype",
        "favourites",
        "figsize",
        "giscus",
        "Heavey",
        "iloc",
        "indiv",
        "isin",
        "kevinheavey",
        "linewidth",
        "matplotlib",
        "pyarrow",
        "pyplot",
        "seaborn",
        "subdir",
        "subplan",
        "timeseries",
        "vlines",
        "xlabel",
        "ylabel",
        "ylim"
    ],
    "cSpell.ignorePaths": [
        "package-lock.json",
        "node_modules",
        "vscode-extension",
        ".git/objects",
        ".vscode",
        ".vscode-insiders",
        "*.ipynb"
    ],
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/.hg/store/**": true,
        "**/.dart_tool": true,
        "**/exp*/**": true,
        "**/*__audio__*/**": true,
        "**/*__raw__*/**": true,
        "**/*__subsets__*/**": true,
        "**/*__refs__*/**": true,
        "**/*__media__*/**": true,
        "**/*__pycache__*/**": true,
        "**/*data_sets*/**": true,
        "**/.git/**": true,
        "**/node_modules/**": true,
        "**/.vscode/**": true
    }
}